alias dots="dotbare"
cpdots() {
  [[ -f "$1" ]] || return 1
  mkdir -p "$HOME/.dots/conf$(dirname $(realpath "$1"))"
  newpath="$HOME/.dots/conf$(realpath "$1")"
  cp -v "$1" "$newpath"
  dots add "$newpath"
}

alias ls="ls --color=auto"

alias g="git"
alias gp="g push"
alias gss="g status --short"
alias gco="g checkout"
alias gcob="gco -b"
alias gc="g commit"
alias gcm="gc -m"
alias gcav="gc -av"
alias grh="g reset --hard"
alias gll="g log"
alias gl="gll -1 HEAD"
alias glo="gll --oneline -n 10"
alias gu="g pull"
alias ga="g add"
alias gaa="ga --all"

eval "$(thefuck --alias)"
